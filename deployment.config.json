{
  "domain": "chimaridata.com",
  "buildCommand": "npm run build",
  "startCommand": "npm start",
  "publicDir": "client/dist",
  "env": {
    "NODE_ENV": "production",
    "PORT": "5000"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}